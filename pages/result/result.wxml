<view class="container">
    <ec-canvas id="mychart-dom-bar" canvas-id="mychart-bar" ec="{{ec}}"></ec-canvas>
    <view class="page-body">
        <view class="page-section">
            <view class="weui-form-preview">
                <view class="weui-form-preview__hd">
                    <view class="weui-form-preview__item">
                        <view class="weui-form-preview__label">税后工资</view>
                        <view class="weui-form-preview__value_in-hd">
                            {{results.afterTaxIncome}}元
                        </view>
                    </view>
                </view>
                <view class="weui-form-preview__bd">
                    <view class="weui-form-preview__item">
                        <view class="weui-form-preview__label">税前工资</view>
                        <view class="weui-form-preview__value">
                            {{results.preTaxIncome}}元
                        </view>
                    </view>
                    <view class="weui-form-preview__item">
                        <view class="weui-form-preview__label">个人所得税</view>
                        <view class="weui-form-preview__value">
                            {{results.personalIncomeTax}}元
                        </view>
                    </view>
                    <view class="weui-form-preview__item">
                        <view class="weui-form-preview__label">医疗保险</view>
                        <view class="weui-form-preview__value">
                            {{results.personalMedical}}元
                        </view>
                    </view>
                    <view class="weui-form-preview__item">
                        <view class="weui-form-preview__label">养老保险</view>
                        <view class="weui-form-preview__value">
                            {{results.personalEndowment}}元
                        </view>
                    </view>
                    <view class="weui-form-preview__item">
                        <view class="weui-form-preview__label">失业保险</view>
                        <view class="weui-form-preview__value">
                            {{results.personalUnemployment}}元
                        </view>
                    </view>
                    <view class="weui-form-preview__item">
                        <view class="weui-form-preview__label">公积金</view>
                        <view class="weui-form-preview__value">
                            {{results.personalHousingFund}}元
                        </view>
                    </view>
                </view>
            </view>
            <view class="weui-cells__tips">注：由于各地工伤保险有差异，计算结果仅供参考</view>
        </view>
        <view class="page-section page-section-gap">
            <ad unit-id="adunit-3833c4aefdfc1219"></ad>
        </view>
    </view>
    <view class="page-foot">
        <button class="weui-btn" type="primary" bindtap="goBack">
            返回
        </button>
        <button class="weui-btn" type="primary" plain="true" open-type="contact">
            意见反馈
        </button>
    </view>
</view>
